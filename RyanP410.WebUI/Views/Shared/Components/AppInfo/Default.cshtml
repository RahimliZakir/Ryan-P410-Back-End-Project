@model AppInfo

<div class="row">
    <div class="col col-d-12 col-t-12 col-m-12 border-line-v">
        <div class="iframe-map">
            @Html.Raw(Model.Map)
        </div>
        <div class="info-list">
            <ul>
                <li><strong>Address . . . . .</strong> @Model.Address</li>
                <li><strong>Email . . . . .</strong> @Model.Email</li>
                <li><strong>Phone . . . . .</strong> @Model.PhoneNumber</li>
                <li><strong>Freelance . . . . .</strong> @(Model.IsFreelance ? "Freelance" : "Not Freelance")</li>
            </ul>
        </div>
    </div>
    <div class="clear"></div>
</div>